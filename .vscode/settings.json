{
  "remote.containers.dotfiles.repository": "https://github.com/yukin01/dotfiles",
  "remote.containers.dotfiles.targetPath": "~/dotfiles",
  "remote.containers.dotfiles.installCommand": "~/dotfiles/install-in-remote-containers.sh",
  "files.associations": {
    "iostream": "cpp",
    "ostream": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "*.tcc": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "string": "cpp",
    "vector": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "string_view": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "scoped_allocator": "cpp",
    "regex": "cpp",
    "tuple": "cpp",
    "bitset": "cpp"
  },
  // "[cpp]": {
  //   "editor.formatOnSave": true
  // }
  "editor.formatOnSave": true,
  "C_Cpp.default.cppStandard": "c++17"
}
